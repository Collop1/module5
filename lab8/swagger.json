{
    "swagger": "2.0",
    "info": {
        "version": "1.0.0",
        "title": "Calculator API",
        "description": "A simple calculator API with basic arithmetic operations",
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        }
    },
    "host": "localhost:3000",
    "basePath": "/",
    "tags": [
        {
            "name": "Calculator",
            "description": "API for performing arithmetic operations"
        }
    ],
    "schemes": [
        "http"
    ],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/calculator/add": {
            "get": {
                "tags": [
                    "Calculator"
                ],
                "summary": "Add two numbers",
                "description": "Returns the sum of two numbers",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "description": "The first number",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "description": "The second number",
                        "required": true,
                        "type": "integer"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "result": {
                                    "type": "integer",
                                    "description": "The sum of the two numbers"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid input parameters",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string",
                                    "description": "Error message"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Route not found"
                    }
                }
            }
        },
        "/calculator/subtract": {
            "get": {
                "tags": [
                    "Calculator"
                ],
                "summary": "Subtract two numbers",
                "description": "Returns the difference between two numbers",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "description": "The first number",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "description": "The second number",
                        "required": true,
                        "type": "integer"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "result": {
                                    "type": "integer",
                                    "description": "The difference between the two numbers"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid input parameters",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string",
                                    "description": "Error message"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Route not found"
                    }
                }
            }
        },
        "/calculator/multiply": {
            "get": {
                "tags": [
                    "Calculator"
                ],
                "summary": "Multiply two numbers",
                "description": "Returns the product of two numbers",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "description": "The first number",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "description": "The second number",
                        "required": true,
                        "type": "integer"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "result": {
                                    "type": "integer",
                                    "description": "The product of the two numbers"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid input parameters",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string",
                                    "description": "Error message"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Route not found"
                    }
                }
            }
        },
        "/calculator/divide": {
            "get": {
                "tags": [
                    "Calculator"
                ],
                "summary": "Divide two numbers",
                "description": "Returns the quotient of two numbers",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "description": "The dividend (number to be divided)",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "description": "The divisor (number to divide by)",
                        "required": true,
                        "type": "integer"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "result": {
                                    "type": "number",
                                    "format": "float",
                                    "description": "The quotient of the division"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid input parameters or division by zero",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string",
                                    "description": "Error message"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Route not found"
                    }
                }
            }
        }
    }
}